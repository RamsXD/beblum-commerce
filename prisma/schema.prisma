generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
  
}

datasource db {
  provider = "postgresql"
}

model User {
  id       Int      @id @default(autoincrement())
  externalId String  @unique
  attributes Json
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
  stripeCustomerId String?
}
